<?xml version="1.0" encoding="UTF-8" ?>
<simulation>
    <type>Schelling Segregation</type>
    <title>Edge-Based Migration</title>
    <author>Angela Predolac</author>
    <description>Agents move towards the edges as segregation progresses.</description>
    <width>6</width>
    <height>6</height>
    <parameter name="satisfaction" value="0.5"/>
    <!-- 0 : EMPTY      1 : TYPE A      2 : TYPE B -->
    <initial_states>
        0 0 1 2 0 0
        1 0 0 0 2 1
        0 1 0 2 0 0
        0 0 2 0 1 0
        2 0 0 0 0 2
        0 2 1 0 0 0
    </initial_states>
</simulation>

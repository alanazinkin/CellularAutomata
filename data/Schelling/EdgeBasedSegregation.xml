<?xml version="1.0" encoding="UTF-8" ?>
<simulation>
    <type>Schelling Segregation</type>
    <title>Edge-Based Segregation</title>
    <author>Angela Predolac</author>
    <description>Agents segregate along the edges of the grid.</description>
    <width>6</width>
    <height>6</height>
    <parameter name="satisfaction" value="0.4"/>
    <!-- 0 : EMPTY      1 : TYPE A      2 : TYPE B -->
    <initial_states>
        1 1 1 0 2 2
        1 1 0 0 2 2
        1 0 0 0 0 2
        1 0 0 0 0 2
        1 1 0 0 2 2
        1 1 1 0 2 2
    </initial_states>
</simulation>

<?xml version="1.0" encoding="UTF-8" ?>
<simulation>
  <type>Spreading of Fire</type>
  <title>Basic Center Fire Spread</title>
  <author>Angela Predolac</author>
  <description>This simulation models a forest fire, where trees catch fire and burn based on neighboring cells</description>
  <width>5</width>
  <height>5</height>
  <parameters>
    <parameter name="treeProb" value="0.900000"/>
    <parameter name="fireProb" value="0.700000"/>
  </parameters>
  <grid>
    BURNT,BURNING,TREE,BURNING,TREE
    TREE,BURNT,BURNT,BURNT,BURNT
    BURNT,BURNT,BURNT,BURNING,BURNING
    TREE,BURNT,BURNT,BURNING,BURNT
    BURNING,BURNT,BURNT,TREE,TREE
  </grid>
</simulation>
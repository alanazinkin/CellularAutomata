<?xml version="1.0" encoding="UTF-8" ?>
<simulation>
    <type>Wa-Tor World</type>
    <title>Basic Predator-Prey</title>
    <author>Angela Predolac</author>
    <description>Balanced fish and shark population with default reproduction and starvation rates.</description>
    <width>6</width>
    <height>6</height>
    <parameter name="fishBreedTime" value="3"/>
    <parameter name="sharkBreedTime" value="5"/>
    <parameter name="sharkInitialEnergy" value="10"/>
    <parameter name="sharkEnergyGain" value="2"/>
    <!-- 0 : EMPTY      1 : FISH      2 : SHARK -->
    <initial_states>
        1 0 2 0 1 2
        0 1 2 1 0 2
        2 1 0 2 1 0
        0 2 1 0 2 1
        1 0 2 1 0 2
        0 1 0 2 1 0
    </initial_states>
</simulation>

<?xml version="1.0" encoding="UTF-8" ?>
<simulation>
    <type>Wa-Tor World</type>
    <title>Cyclical Predator-Prey Equilibrium</title>
    <author>Angela Predolac</author>
    <description>Fish and sharks are positioned to maintain a natural predator-prey cycle.</description>
    <width>6</width>
    <height>6</height>
    <parameter name="fishBreedTime" value="3"/>
    <parameter name="sharkBreedTime" value="4"/>
    <parameter name="sharkInitialEnergy" value="3"/>
    <parameter name="sharkEnergyGain" value="3"/>
    <initial_states>
        1 0 2 0 1 0
        0 2 0 1 0 2
        2 0 1 0 2 0
        0 1 0 2 0 1
        1 0 2 0 1 0
        0 2 0 1 0 2
    </initial_states>
</simulation>

<?xml version="1.0" encoding="UTF-8" ?>
<simulation>
    <type>Wa-Tor World</type>
    <title>Overpopulated Sharks</title>
    <author>Angela Predolac</author>
    <description>Too many sharks will quickly run out of food and die off.</description>
    <width>6</width>
    <height>6</height>
    <parameter name="fishBreedTime" value="3"/>
    <parameter name="sharkBreedTime" value="6"/>
    <parameter name="sharkInitialEnergy" value="2"/>
    <parameter name="sharkEnergyGain" value="2"/>
    <initial_states>
        2 2 2 2 2 2
        2 2 2 2 2 2
        2 2 1 1 2 2
        2 2 1 1 2 2
        2 2 2 2 2 2
        2 2 2 2 2 2
    </initial_states>
</simulation>
